@{
    ViewBag.Title = "Inicio";
}

<!-- Page-Title -->

<div class="row">
    <div class="col-sm-12">
        <h4 class="page-title">@ViewBag.Title</h4>
    </div>
</div>

<div class="row">
    <div class="col-sm-12">
        <div class="container">
            <div class="row">
                <div class="masonry-container">
                    <div class="col-md-6 col-lg-4 masonry-column">
                        <div class="card-box">
                            <h3 class="panel-title m-t-0 text-right text-custom"><b>14 DE OCTUBRE</b></h3>
                            <p class="text-muted m-b-20 font-13 text-right">
                                Hora: 10:00 a 15:00
                            </p>
                            <img src="https://academico.quitec.com.ec/cas/assets/images/cursos/taller-de-cocina-brasileña.jpg" class="img-responsive" />
                        </div>
                    </div>

                    <div class="col-md-6 col-lg-4 masonry-column">
                        <div class="card-box">
                            <h3 class="panel-title m-t-0 text-right text-custom"><b>21 DE OCTUBRE</b></h3>
                            <p class="text-muted m-b-20 font-13 text-right">
                                Hora: 10:00 a 15:00
                            </p>
                            <img src="https://academico.quitec.com.ec/cas/assets/images/cursos/taller-de-colada-morada.jpg" class="img-responsive" />
                        </div>
                    </div>
                    <div class="col-md-6 col-lg-4">
                        <div class="card-box">
                            <h3 class="panel-title m-t-0 text-right text-custom"><b>23 DE SEPTIEMBRE</b></h3>
                            <p class="text-muted m-b-20 font-13 text-right">
                                Hora: 10:00 a 15:00
                            </p>
                            <img src="https://academico.quitec.com.ec/cas/assets/images/cursos/1.png" class="img-responsive" />

                        </div>
                    </div>
                    <div class="col-md-6 col-lg-4 masonry-column">
                        <div class="card-box">
                            <h3 class="panel-title m-t-0 text-right text-custom"><b>30 DE SEPTIEMBRE</b></h3>
                            <p class="text-muted m-b-20 font-13 text-right">
                                Hora: 10:00 a 15:00
                            </p>
                            <img src="https://academico.quitec.com.ec/cas/assets/images/cursos/2.png" class="img-responsive" />
                        </div>
                    </div>

                    <div class="col-md-6 col-lg-4 masonry-column">
                        <div class="card-box">
                            <h3 class="panel-title m-t-0 text-right text-custom"><b>23 DE SEPTIEMBRE</b></h3>
                            <p class="text-muted m-b-20 font-13 text-right">
                                Hora: 10:00 a 15:00
                            </p>
                            <img src="https://academico.quitec.com.ec/cas/assets/images/cursos/3.png" class="img-responsive" />
                        </div>
                    </div>

                </div>
            </div>
            <!-- End row -->

        </div>
        <!-- end row -->
    </div>
</div>



@section Scripts
{
    <script>
        $(window).load(function(){
            var $container = $('.masonry-container');
            $container.isotope({
                filter: '*'
            });
        });

    </script>
}